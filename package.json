{
    "name": "@patrick115/sveltekitapi",
    "version": "1.2.16",
    "description": "Library for creating API endpoints in SvelteKit",
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "scripts": {
        "dev": "tsx watch ./src/index.ts",
        "build": "mkdir -p build && tsc",
        "clear": "rm -rf build",
        "check": "tsc --noEmit",
        "format": "prettier --write .",
        "lint": "prettier --check . && eslint .",
        "publ": "npm publish --access public",
        "prepare": "husky"
    },
    "keywords": [],
    "author": "Patrik MintÄ›l",
    "license": "MIT",
    "type": "module",
    "dependencies": {
        "@sveltejs/kit": "^2.31.1",
        "zod": "^4.0.17"
    },
    "devDependencies": {
        "@eslint/compat": "^1.3.2",
        "@eslint/js": "^9.33.0",
        "@types/eslint": "^9.6.1",
        "@types/node": "^24.3.0",
        "@types/node-fetch": "^2.6.13",
        "eslint": "^9.33.0",
        "eslint-config-prettier": "^10.1.8",
        "husky": "^9.1.7",
        "lint-staged": "^16.1.5",
        "prettier": "^3.6.2",
        "tsx": "^4.20.4",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.39.1"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/patrick11514/SveltekitAPIHandler.git"
    },
    "bugs": {
        "url": "https://github.com/patrick11514/SveltekitAPIHandler/issues"
    },
    "homepage": "https://github.com/patrick11514/SveltekitAPIHandler#readme",
    "lint-staged": {
        "*.ts": [
            "eslint --fix",
            "prettier --write"
        ]
    }
}
